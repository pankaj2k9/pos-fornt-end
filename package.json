{
  "name": "the-odbo-pos",
  "description": "The Odbo POS",
  "license": "UNLICENSED",
  "private": true,
  "author": {
    "name": "Gwill Web Services"
  },
  "engines": {
    "node": ">= 5.0.0"
  },
  "scripts": {
    "setup": "node tools/setup/setupMessage.js && npm install && node tools/setup/setup.js",
    "start-message": "babel-node tools/startMessage.js",
    "prestart": "npm-run-all --parallel start-message remove-dist",
    "start": "npm-run-all --parallel test:watch open:src lint",
    "open:src": "babel-node tools/srcServer.js",
    "open:dist": "babel-node tools/distServer.js",
    "lint": "eslint webpack.config.* src/**/*.js tools/**/*.js --color --cache --ignore-path .gitignore",
    "lint:staged": "lint-staged",
    "clean-dist": "npm run remove-dist && mkdir dist",
    "remove-dist": "rimraf ./dist",
    "prebuild": "npm run clean-dist && npm run lint && npm run test",
    "build": "babel-node tools/build.js",
    "test": "mocha tools/testSetup.js \"src/**/*.spec.js\" --reporter progress",
    "test:cover": "babel-node node_modules/isparta/bin/isparta cover --root src --report html node_modules/mocha/bin/_mocha -- --require ./tools/testSetup.js \"src/**/*.spec.js\" --reporter progress",
    "test:cover:travis": "babel-node node_modules/isparta/bin/isparta cover --root src --report lcovonly _mocha -- --require ./tools/testSetup.js \"src/**/*.spec.js\" && cat ./coverage/lcov.info | node_modules/coveralls/bin/coveralls.js",
    "test:watch": "npm run test -- --watch"
  },
  "dependencies": {
    "bcryptjs": "2.4.3",
    "bulma": "^0.3.1",
    "feathers-authentication-client": "^0.1.6",
    "feathers-client": "^1.9.0",
    "file-saver": "^1.3.3",
    "font-awesome": "^4.7.0",
    "intl": "1.2.5",
    "moment": "2.18.1",
    "object-assign": "^4.1.1",
    "offline-plugin": "^4.5.5",
    "react": "15.6.1",
    "react-addons-shallow-compare": "15.6.0",
    "react-dates": "12.3.0",
    "react-dom": "15.6.1",
    "react-input-enhancements": "0.5.4",
    "react-intl": "^2.2.3",
    "react-intl-redux": "0.5.0",
    "react-redux": "^5.0.2",
    "react-router": "2.8.1",
    "react-router-redux": "^4.0.7",
    "redux": "3.6.0",
    "redux-logger": "^2.7.4",
    "redux-thunk": "^2.2.0",
    "shortid": "2.2.8",
    "whatwg-fetch": "^2.0.2"
  },
  "devDependencies": {
    "babel-cli": "^6.22.2",
    "babel-core": "^6.22.1",
    "babel-eslint": "^7.1.1",
    "babel-loader": "^6.2.10",
    "babel-plugin-react-display-name": "2.0.0",
    "babel-preset-es2015": "^6.22.0",
    "babel-preset-react": "^6.22.0",
    "babel-preset-react-hmre": "1.1.1",
    "babel-preset-stage-1": "^6.22.0",
    "babel-register": "^6.22.0",
    "browser-sync": "^2.18.6",
    "chai": "3.5.0",
    "chalk": "1.1.3",
    "connect-history-api-fallback": "1.3.0",
    "coveralls": "^2.11.15",
    "cross-env": "^3.1.4",
    "css-loader": "^0.26.1",
    "enzyme": "^2.7.1",
    "eslint": "^3.14.0",
    "eslint-config-standard": "^6.2.1",
    "eslint-config-standard-jsx": "3.2.0",
    "eslint-plugin-promise": "^3.4.0",
    "eslint-plugin-react": "^6.9.0",
    "eslint-plugin-standard": "2.0.1",
    "extract-text-webpack-plugin": "1.0.1",
    "file-loader": "0.9.0",
    "html-webpack-plugin": "^2.26.0",
    "isparta": "4.0.0",
    "lint-staged": "^3.2.7",
    "mocha": "^3.2.0",
    "node-sass": "^4.3.0",
    "npm-run-all": "^4.0.1",
    "pre-commit": "^1.2.2",
    "prompt": "1.0.0",
    "react-addons-test-utils": "^15.4.2",
    "redux-immutable-state-invariant": "1.2.4",
    "replace": "0.3.0",
    "rimraf": "2.5.4",
    "sass-loader": "^4.1.1",
    "sinon": "^1.17.7",
    "sinon-chai": "2.8.0",
    "style-loader": "0.13.1",
    "url-loader": "0.5.7",
    "webpack": "^1.14.0",
    "webpack-dev-middleware": "1.11.0",
    "webpack-hot-middleware": "2.18.2",
    "webpack-md5-hash": "0.0.5"
  },
  "lint-staged": {
    "*.js": "eslint --rule 'no-console: 2'"
  },
  "pre-commit": "lint:staged",
  "eslintConfig": {
    "env": {
      "jquery": true
    },
    "extends": [
      "standard",
      "standard-jsx"
    ],
    "parser": "babel-eslint",
    "ignore": [
      "./build"
    ]
  }
}
